{{>partials/header}}
        <title>Search Engine Results</title>
        <link rel="stylesheet" href="/css/styleResult.css">
    </head>
    <body>
        <div class="container">
            <div id="StandardSearchBar">
                {{>partials/resultPageSearch}}
            </div>
            <nav id="nav-searchBar">
                {{>partials/resultPageSearch}}
            </nav>

            <div class="row bodyResult">
                {{#newsList}}
                    <div class="col-sm-12 col-md-7 col-lg-7 col-xl-7 col-xxl-7">
                        <a href="{{link}}" data-bs-toggle="tooltip" data-bs-placement="bottom" title="{{link}}">{{title}}</a>
                        <h6>{{link}}</h6>
                        <p>{{description}}</p>
                    </div>
                {{/newsList}}
            </div>
            
            <div class="row" id="paginationResult">
                <nav aria-label="Page navigation">
                    <ul class="pagination">
                        <li class="page-item">
                            <a class="page-link" href="#" aria-label="Previous" >
                                <span aria-hidden="true">&laquo;</span>
                            </a>
                        </li>
                        <li class="page-item active"><a class="page-link" href="#">1</a></li>
                        <li class="page-item"><a class="page-link" href="#">2</a></li>
                        <li class="page-item"><a class="page-link" href="#">3</a></li>
                        <li class="page-item">
                            <a class="page-link" href="#" aria-label="Next">
                                <span aria-hidden="true">&raquo;</span>
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>
        </div>
    
{{>partials/footer}}